{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "DatalakeRestorePermissions",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::<BACKUP_BUCKET>/*",
        "arn:aws:s3:::<BACKUP_BUCKET>"
      ]
    }
  ]
}
